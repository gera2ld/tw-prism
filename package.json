{
  "name": "tw-prism",
  "version": "0.1.0",
  "description": "TiddlyWiki plugin to provide syntax highlight with PrismJS",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "plaid develop",
    "build:tool": "plaid build -k",
    "build:js": "node scripts/build.mjs",
    "build:tw": "TIDDLYWIKI_PLUGIN_PATH=dist tiddlywiki dist/data --build",
    "build": "run-s build:js build:tw build:tool"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "prismjs": "^1.24.1"
  },
  "devDependencies": {
    "@gera2ld/plaid": "^2.3.4-alpha.0",
    "@gera2ld/plaid-webpack": "^2.3.4-alpha.2",
    "@gera2ld/plaid-webpack-svelte": "^2.3.4-alpha.0",
    "del": "^6.0.0",
    "svelte": "^3.41.0",
    "tiddlywiki": "^5.1.23"
  },
  "repository": "git@github.com:gera2ld/tw-prism.git"
}
